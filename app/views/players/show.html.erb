<%= link_to("<< Back to index", {:action => 'index'}, :class => 'back-link') %> 

<div class = "show player">
	<h2>Player Details</h2>
	<table>
			<tr>
				<th>First Name</th>
				<td><%= @player.first_name %></td>
			</tr>
			<tr>
				<th>Last Name</th>
				<td><%= @player.last_name %></td>
			</tr>
			<tr>
				<th>Email</th>
				<td><%= @player.email %></td>
			</tr>
			<tr>
				<th>Team</th>
				<td><%= @player.team.name if !@player.team.nil? %></td>
			</tr>
	</table>
</div>
